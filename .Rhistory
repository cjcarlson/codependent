curve.df <- function(assoc.df, iter){
u.hosts <- unique(assoc.df$Host)
n.hosts <- length(u.hosts)
results.df <- data.frame(n.host=0,n.par=0)
results.df <- results.df[-1,]
for (i in 1:n.hosts) {
#for (i in 1:5) {
